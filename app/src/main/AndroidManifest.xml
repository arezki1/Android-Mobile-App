<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arezki.myproject">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Pronounce_Activity" />
        <activity android:name=".Count_Activity" />
        <activity android:name=".Order_Activity" />
        <activity android:name=".Count_three" />
        <activity android:name=".Count_one" />
        <activity android:name=".Count_four" />
        <activity android:name=".Count_five" />
        <activity android:name=".Count_two"></activity>
    </application>

</manifest>